{
  "schemaVersion": 1,
  "id": "affinity",
  "version": "${version}",
  "name": "Affinity",
  "description": "A magic mod themed around natural tech",
  "authors": [
    "glisco"
  ],
  "contributors": [
    "naschhorn",
    "Pois1x",
    "pvpTurtle",
    "Aikoyori",
    "BasiqueEvangelist",
    "chyzman",
    "Noaaan"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/affinity/icon.png",
